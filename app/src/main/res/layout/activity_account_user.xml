<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Account_user">

    <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="100dp" android:layout_height="100dp"

            android:src="@drawable/ic_person_black_24dp"

            tools:ignore="VectorDrawableCompat" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:id="@+id/circleImageView"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_id"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/circleImageView" android:layout_marginTop="20dp"
            android:hint="User Id" android:textSize="18sp" android:textStyle="bold"
            android:textColor="@android:color/background_dark"/>
    <View
            android:layout_width="match_parent"
            android:layout_height="5dp" android:id="@+id/view"
            app:layout_constraintTop_toBottomOf="@+id/u_id" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@android:color/background_dark" android:layout_marginTop="20dp"/>
    <View
            android:layout_width="match_parent"
            android:layout_height="5dp" android:id="@+id/view2"
            app:layout_constraintTop_toBottomOf="@+id/u_id" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@android:color/background_dark" android:layout_marginTop="280dp"/>
    <TextView
            android:text="First Name  :-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:textSize="14sp"
            android:textColor="@android:color/background_dark" android:textStyle="bold"
            android:layout_marginTop="40dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_f_name"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintHorizontal_bias="0.0"
            android:textColor="@android:color/background_dark" android:textStyle="bold" android:textSize="14sp"
            android:layout_marginTop="40dp"/>
    <TextView
            android:text="Last Name  :-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView25"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:textSize="14sp"
            android:textColor="@android:color/background_dark" android:textStyle="bold"
            android:layout_marginTop="80dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_l_name"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintHorizontal_bias="0.0"
            android:textColor="@android:color/background_dark" android:textStyle="bold" android:textSize="14sp"
            android:layout_marginTop="80dp"/>
    <TextView
            android:text="Contact No  :-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView23"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:textSize="14sp"
            android:textColor="@android:color/background_dark" android:textStyle="bold"
            android:layout_marginTop="110dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_mob"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintHorizontal_bias="0.0"
            android:textColor="@android:color/background_dark" android:textStyle="bold" android:textSize="14sp"
            android:layout_marginTop="110dp"/>
    <TextView
            android:text="Address  :-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView27"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:textSize="14sp"
            android:textColor="@android:color/background_dark" android:textStyle="bold"
            android:layout_marginTop="140dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_address"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintHorizontal_bias="0.0"
            android:textColor="@android:color/background_dark" android:textStyle="bold" android:textSize="14sp"
            android:layout_marginTop="140dp"/>
    <TextView
            android:text="Adhar No  :-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView31"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:textSize="14sp"
            android:textColor="@android:color/background_dark" android:textStyle="bold"
            android:layout_marginTop="170dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_adhar"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintHorizontal_bias="0.0"
            android:textColor="@android:color/background_dark" android:textStyle="bold" android:textSize="14sp"
            android:layout_marginTop="170dp" android:ellipsize="end" android:ems="13"/>
    <TextView
            android:text="Gender  :-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView29"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:textSize="14sp"
            android:textColor="@android:color/background_dark" android:textStyle="bold"
            android:layout_marginTop="200dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/u_gender"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/view" app:layout_constraintHorizontal_bias="0.0"
            android:textColor="@android:color/background_dark" android:textStyle="bold" android:textSize="14sp"
            android:layout_marginTop="200dp"/>
</android.support.constraint.ConstraintLayout>